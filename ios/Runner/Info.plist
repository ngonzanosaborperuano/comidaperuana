<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>CADisableMinimumFrameDurationOnPhone</key>
		<true/>
		<key>CFBundleDevelopmentRegion</key>
		<string>$(DEVELOPMENT_LANGUAGE)</string>
		<key>CFBundleDisplayName</key>
		<string>Cocinando</string>
		<key>CFBundleExecutable</key>
		<string>$(EXECUTABLE_NAME)</string>
		<key>CFBundleIdentifier</key>
		<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
		<key>CFBundleInfoDictionaryVersion</key>
		<string>6.0</string>
		<key>CFBundleName</key>
		<string>Cocinando</string>
		<key>CFBundlePackageType</key>
		<string>APPL</string>
		<key>CFBundleShortVersionString</key>
		<string>$(FLUTTER_BUILD_NAME)</string>
		<key>CFBundleSignature</key>
		<string>????</string>
		<key>CFBundleVersion</key>
		<string>$(FLUTTER_BUILD_NUMBER)</string>
		<key>FLTEnableImpeller</key>
		<false/>
		<key>FlutterDeepLinkingEnabled</key>
		<true/>

		<!-- URL Schemes (Google Sign-In, etc.) -->
		<key>CFBundleURLTypes</key>
		<array>
			<dict>
				<key>CFBundleURLName</key>
				<string>cocinando.shop</string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>ricope</string>
				</array>
			</dict>
			<dict>
				<key>CFBundleTypeRole</key>
				<string>Editor</string>
				<key>CFBundleURLName</key>
				<string></string>
				<key>CFBundleURLSchemes</key>
				<array>
					<string>com.googleusercontent.apps.615495880468-vaqqmb8h3ihto2np80mbhthuvtovjilu</string>
				</array>
			</dict>
		</array>

		<!-- Google Sign-In -->
		<key>GIDClientID</key>
		<string>615495880468-vaqqmb8h3ihto2np80mbhthuvtovjilu.apps.googleusercontent.com</string>
		<key>GoogleSignIn</key>
		<dict>
			<key>ClientID</key>
			<string>615495880468-vaqqmb8h3ihto2np80mbhthuvtovjilu.apps.googleusercontent.com</string>
			<key>ReversedClientID</key>
			<string>com.googleusercontent.apps.615495880468-vaqqmb8h3ihto2np80mbhthuvtovjilu</string>
		</dict>

		<!-- Permisos del sistema -->
		<key>NSCameraUsageDescription</key>
		<string>Esta aplicación necesita acceso a la cámara para tomar fotos a los platos o ingredientes.</string>
		<key>NSPhotoLibraryUsageDescription</key>
		<string>Esta aplicación necesita acceso a las fotos para guardar o cargar imágenes de tus platos.</string>
		<key>NSMicrophoneUsageDescription</key>
		<string>Esta aplicación necesita acceso al micrófono para grabar instrucciones o audios de recetas.</string>
		<key>NSLocationWhenInUseUsageDescription</key>
		<string>Necesitamos tu ubicación para sugerirte ingredientes o platos cercanos.</string>
		<key>NSLocationAlwaysUsageDescription</key>
		<string>Necesitamos tu ubicación incluso en segundo plano para enviarte alertas de ingredientes cercanos.</string>

		<!-- Permisos de red local para evitar error del observatorio -->
		<key>NSLocalNetworkUsageDescription</key>
		<string>Esta aplicación requiere acceso a la red local para funciones de desarrollo y depuración.</string>
		<key>NSBonjourServices</key>
		<array>
			<string>_dartobservatory._tcp</string>
		</array>

		<!-- Red arbitraria (desarrollo) -->
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<true/>
		</dict>

		<!-- Notificaciones -->
		<key>UIBackgroundModes</key>
		<array>
			<string>fetch</string>
			<string>remote-notification</string>
		</array>

		<!-- Orientaciones -->
		<key>UISupportedInterfaceOrientations</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>
		<key>UISupportedInterfaceOrientations~ipad</key>
		<array>
			<string>UIInterfaceOrientationPortrait</string>
			<string>UIInterfaceOrientationPortraitUpsideDown</string>
			<string>UIInterfaceOrientationLandscapeLeft</string>
			<string>UIInterfaceOrientationLandscapeRight</string>
		</array>

		<key>UILaunchStoryboardName</key>
		<string>LaunchScreen</string>
		<key>UIMainStoryboardFile</key>
		<string>Main</string>

		<!-- Consultas de esquema -->
		<key>LSApplicationQueriesSchemes</key>
		<array>
			<string>pulsesecure</string>
		</array>

		<key>LSRequiresIPhoneOS</key>
		<true/>

		<!-- Permisos de notificación (legacy, opcional) -->
		<key>UIUserNotificationType</key>
		<dict>
			<key>UIUserNotificationTypeAlert</key>
			<string>YES</string>
			<key>UIUserNotificationTypeBadge</key>
			<string>YES</string>
			<key>UIUserNotificationTypeSound</key>
			<string>YES</string>
		</dict>

		<key>UIApplicationSupportsIndirectInputEvents</key>
		<true/>
	</dict>
</plist>
